{
  "name": "reselect-tool-extension",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "build:chrome": "cpx 'chrome/**/*.{html,js,json}' build",
    "build:app": "parcel build src/index.js -d build -o panel.js",
    "watch:chrome": "cpx 'chrome/**/*.{html,js,json}' build --watch",
    "watch:app": "parcel watch src/index.js -d build -o panel.js",
    "build": "concurrently 'yarn build:chrome' 'yarn build:app'",
    "watch": "concurrently 'yarn watch:chrome' 'yarn watch:app'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "devextreme": "19.2.5",
    "devextreme-react": "19.2.5",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "concurrently": "5.1.0",
    "cpx": "1.5.0",
    "parcel-bundler": "1.12.4"
  }
}
